<project>
	<name>Transmission LD Test</name>
	<settings>
		<bgColor>#202020</bgColor>
		<defaultWidth>810</defaultWidth>
		<defaultHeight>480</defaultHeight>
		<minWidth>32</minWidth>
		<minHeight>32</minHeight>
		<maxWidth>1000</maxWidth>
		<maxHeight>600</maxHeight>
		<workingDirectory>gfx</workingDirectory>
	</settings>
	
	<objects>
		<object name="player" image="player.png" width="30" height="30"/>
	</objects> 
	
	<tilesets>
		<tileset name="Paths" image="spritesheetBVR.png" tileWidth="30" tileHeight="30"/>
	</tilesets> 
	
	<layers>
	<!-- colors #AARRGGBB-->
		<grid name="mask_red" color="#FFFF0000" gridSize="30" drawGridSize="30" exportAsObjects="true" />
		<tiles name="path_red" gridSize="30" drawGridSize="60" />
		<grid name="mask_green" color="#FF00FF00" gridSize="30" drawGridSize="30" exportAsObjects="true" />
		<tiles name="path_green" gridSize="30" drawGridSize="60" />
		<grid name="mask_blue" color="#FF0000FF" gridSize="30" drawGridSize="30" exportAsObjects="true" />
		<tiles name="path_blue" gridSize="30" drawGridSize="60" />
		<objects name="objects" gridSize="30" drawGridSize="30" />
		
	</layers>
</project>